{
  "name": "medweg-backend",
  "version": "0.1.0",
  "description": "MEDWEG Backend API - B2B Medical Supplies Management System",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "ts-node src/config/migrations/run.ts",
    "seed": "ts-node src/config/seeds/run.ts",
    "script1": "ts-node src/scripts/seedInstitutions.ts",
    "script2": "ts-node src/scripts/seedPatients.ts",
    "script3": "ts-node src/scripts/seedOrders.ts",
    "fix-images": "ts-node src/scripts/fixProductImages.ts"
  },
  "keywords": [
    "medical",
    "b2b",
    "healthcare",
    "api",
    "express",
    "typescript",
    "postgresql"
  ],
  "author": "MEDWEG Team",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@types/nodemailer": "^7.0.3",
    "aws-sdk": "^2.1515.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.10",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "web-push": "^3.6.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.13.9",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
